import random


class Card:

# 2) Create the class constructor. Use the following method comment.
    def __init__(self):
        self.value1= (0)
        self.value2= (0)

# 3) Create the roll(self) method. Use the following method comment.
    def roll1(self):
        self.value1 = random.randint (1,13)

    def roll2(self):
        self.value2 = random.randint (1,13)
